<div id="layout_login">

<%= form_tag login_path do %>
  <div class="field">
    <%= label_tag :username %>
    <%= text_field_tag :username, params[:username] %>
  </div>
  <div class="field">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
  <div class="actions"><%= submit_tag "Acessar" %></div>
 
<% end %>

<%= form_tag users_path do %>
 <div class="actions"><%= submit_tag "Cadastre-se Aqui" %></div>
<% end %>
</div>

